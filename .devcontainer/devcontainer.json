{
  "name": "Sail DevContainer",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
  },
  "postCreateCommand": "hatch env create default",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "ms-python.python"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "python.defaultInterpreterPath": "/usr/bin/python3"
      }
    }
  },
  "remoteUser": "vscode"
}
