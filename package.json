{
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "vue-tsc --noEmit && eslint docs",
    "format": "prettier ${FORMAT_OPTIONS---write} \"*.{js,json,yml,yaml,md}\" \".github/**/*.{yml,yaml,cjs}\" \"docs/**/*.{ts,mts,vue,css,md,json,yml,yaml}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@viz-js/viz": "^3.16.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.3.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prettier-plugin-vitepress": "^0.0.10",
    "sql-formatter": "^15.6.6",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vega": "^6.1.2",
    "vite": "<6",
    "vitepress": "^1.6.3",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.4"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss",
      "prettier-plugin-vitepress"
    ],
    "importOrder": [
      "<BUILT_IN_MODULES>",
      "",
      "<TYPES>",
      "<THIRD_PARTY_MODULES>",
      "",
      "<TYPES>^@theme/",
      "<TYPES>^[.]",
      "^@theme/",
      "^[.]"
    ]
  },
  "packageManager": "pnpm@10.12.3"
}
